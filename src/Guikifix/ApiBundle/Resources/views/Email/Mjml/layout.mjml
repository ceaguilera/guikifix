<!-- USERD1 -->
<mjml>
  <mj-head>
    <mj-font name="Roboto" href="https://fonts.googleapis.com/css?family=Roboto:400,700" />
    <mj-attributes>
      <mj-all font-family="Roboto, Helvetica, sans-serif" />
      <mj-section padding="0" />
    </mj-attributes>
  </mj-head>

  <mj-body>
    <mj-container background-color="#ededed" >
      <mj-section padding-top="4%"></mj-section>
      <mj-wrapper border="1px" background-color="white" padding="0" padding-left="5%" padding-right="5%"  border-radius="10px">
        <!-- HEADER -->
        <mj-section padding-top="2%" background-color="white">
          <mj-column>
            <mj-column>
                <!-- LOGO -->
                <mj-image width="250" src="http://guikifix.herokuapp.com/images/logo_guikifix.png"></mj-image>
                <!-- END LOGO -->
                <mj-image width="500" align="center" src="https://guikifix.herokuapp.com/images/email/line01.png"></mj-image>
              </mj-column>
          </mj-column>
        </mj-section>
        <!-- END HEADER -->
        <!-- BODY -->
        <mj-include path="userc7.mjml" />
        <!-- END BODY -->  
        <!-- BODY TEAM GUIKI -->
        <mj-section padding-bottom="4%">
            <mj-column>
                <mj-text align="center" padding="0">Cualquier duda escríbenos al soporte@guikifix.com.ve </mj-text>
                <mj-text align="center" padding="0">Atentamente,</mj-text>
                <mj-text align="center" padding="0" font-weight="bold">TEAM GUIKIFIX</mj-text>
                <mj-image width="500" align="center" src="https://guikifix.herokuapp.com/images/email/line02.png"></mj-image>
              </mj-column>
        </mj-section>
        <!-- END BODY TEAM GUIKI -->
      </mj-wrapper>
      
      <!-- PRE - FOOTER -->
      <mj-section padding-top="4%"></mj-section>
      <mj-wrapper border="1px" padding="0" background-url="https://guikifix.herokuapp.com/images/email/backgroundfooter.png" padding-left="5%" padding-right="5%"  border-radius="10px" padding-top="6px" padding-bottom="6px">
        <mj-section>
          <mj-image src="http://guikifix.herokuapp.com/images/logo_guikifix.png" padding="0" width="75px"></mj-image>
          <mj-text font-size="9" align="center" font-weight="bold" color="gray">Activa algunos de nuestros Planes profesionales podrás tener acceso a muchísimos trabajos de clientes listos para contratar</mj-text>
          <mj-button background-color="#FF6600" font-weight="bold" font-size="8" color="white">ACTIVA PLAN PROFESIONAL</mj-button>
        </mj-section>  
      </mj-wrapper>
      <!-- END PRE - FOOTER -->
        <!-- FOOTER -->
        <mj-section padding-top="3%" ></mj-section>
        <mj-wrapper border="1px" padding="0" background-color="#094AB1" padding-left="5%" padding-right="5%"  border-radius="10px" padding-top="6px" padding-bottom="6px" >
          <mj-section text-align="center">
            <!--- FIRST FOOTER -->
            <mj-column width="40%">
              <mj-text color="white" font-size="9" align="center" padding="0" font-weight="bold">CONTRATA PROFESIONALES DESDE TU CELULAR</mj-text>
            <mj-text color="white" font-size="9" padding="0" align="center">Descarga nuestra aplicación gratis!</mj-text>
            <mj-text color="white" font-size="9" padding="0" align="center">INNOVACIÓN, TECNOLOGÍA Y SEGURIDAD</mj-text>  
            </mj-column>
            <!---END FIRST FOOTER -->
            <!--- SECOND FOOTER -->
            <mj-column width="20%">
            <mj-text color="white" align="center" font-weight="bold" padding="0">Síguenos</mj-text>
              <mj-table padding-left="23px">
                <tr style="list-style: none;">
                  <td style="padding:0 !important;text-align:center;">
                    <a href="https://twitter.com/RecastAI" style="align:center;">
                      <img width="20px" src="https://guikifix.herokuapp.com/images/email/instagram.png" style="align:center;" />
                    </a>
                  </td>
                  <td style="padding:0 !important;text-align:center;">
                    <a href="https://twitter.com/RecastAI">
                      <img width="20px" src="https://guikifix.herokuapp.com/images/email/facebook.png" style="align:center !important;" />
                    </a>
                  </td>
                  <td style="padding:0 !important;text-align:center;">
                    <a href="https://twitter.com/RecastAI">
                      <img width="20px" src="https://guikifix.herokuapp.com/images/email/twitter.png" style="align:center;" />
                    </a>
                  </td>
                </tr>
              </mj-table>  
            </mj-column>
            <!--- END SECOND FOOTER -->
            <!--- THIRD FOOTER -->
            <mj-column width="40%">
              <mj-section >
                <mj-group>
                  <mj-column>
                    <mj-image width="100px" padding="0" padding-top="4%" src="https://guikifix.herokuapp.com/images/email/googleplay.png"></mj-image>
                  </mj-column>
                  <mj-column>
                    <mj-image width="100px" padding="0" padding-top="4%" src="https://guikifix.herokuapp.com/images/email/appstore.png"></mj-image>
                  </mj-column>
                </mj-group>
              </mj-section>
              <mj-text color="white" padding="0" font-size="7" align="center">Haga click aquí para obtener mas información sobre la aplicación</mj-text>  
            </mj-column>
            <!--- END THIRD FOOTER -->
          </mj-section>
        </mj-wrapper>
        <!-- END FOOTER -->
    </mj-container>
  </mj-body>
</mjml>